{
  "name": "density-back",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/src/index.js",
    "dev": "node --env-file=.env --watch -r ts-node/register src/index.ts",
    "lint": "eslint .",
    "cov:clean": "rimraf .nyc_output && rimraf coverage",
    "cov:unit": "jest --forceExit --colors -c jest.config.unit.js",
    "cov:e2e": "NODE_ENV=e2e jest --detectOpenHandles --forceExit --colors -c jest.config.e2e.js --runInBand",
    "test:unit": "npm-run-all cov:clean cov:unit",
    "test:e2e": "NODE_ENV=e2e npm-run-all cov:clean cov:e2e",
    "test:e2e:ci": "NODE_ENV=ci jest -c ./jest.config.e2e.js --verbose --forceExit --runInBand",
    "migrations:generate": "sequelize-cli migration:generate --name",
    "migrations:run": "sequelize-cli db:migrate",
    "migrations:revert": "sequelize-cli db:migrate:undo",
    "migrations:delete": "sequelize-cli db:migrate:undo:all",
    "migrations:seed:generate": "sequelize-cli seed:generate --name",
    "migrations:run:seed": "sequelize-cli db:seed:all",
    "migrations:seed:revert": "sequelize-cli db:seed:undo",
    "migrations:seed:delete": "sequelize-cli db:seed:undo:all"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/uuid": "^10.0.0",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.11.3",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.13.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.3",
    "sequelize-typescript": "^2.1.6",
    "umzug": "^3.8.2",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.26.0",
    "@eslint/js": "^9.13.0",
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.8.1",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.13.0",
    "globals": "^15.11.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "sequelize-cli": "^6.6.2",
    "sequelize-mock": "^0.10.2",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.11.0"
  }
}
